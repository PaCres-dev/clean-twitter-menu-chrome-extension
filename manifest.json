{
  "manifest_version": 3,
  "name": "Clean Twitter Menu",
  "version": "1.1",
  "description": "Cleans the Twitter menu",
  "icons": {
    "128": "./img/icon.png"
  },
  "action": {
    "default_icon": {
      "128": "./img/icon.png"
    },
    "default_popup": "./popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://x.com/*"],
      "js": ["sidebarCleaner.js"]
    }
  ]
}
